<!DOCTYPE html>
html(lang="en")
  head
    meta(charset="utf-8")
    link(rel="stylesheet", href="/css/main.css")
    link(rel="stylesheet", href="https://fonts.googleapis.com/css?family=Open+Sans|Roboto")
    script(src="https://use.fontawesome.com/c8ca64f559.js")
    script(type='text/javascript', src="/js/dist/jquery.min.js")
    script(type='text/javascript', src="/js/upload.js")
    script(src="https://code.createjs.com/easeljs-0.8.2.min.js")
    script(src="https://code.createjs.com/tweenjs-0.6.2.min.js")
    script(src="http://cdnjs.cloudflare.com/ajax/libs/gsap/1.19.0/TweenMax.min.js")
    script(src="http://cdnjs.cloudflare.com/ajax/libs/gsap/1.19.0/plugins/CSSPlugin.min.js")
    script(src="http://cdnjs.cloudflare.com/ajax/libs/gsap/1.19.0/easing/EasePack.min.js")
    script(src="http://cdnjs.cloudflare.com/ajax/libs/gsap/1.19.0/TweenLite.min.js")
    title= title
    body(onload="init();")
      div.wrapper
        h1= message
        if (todos.length)
          table 
            thead
              tr
                th Identifier
                th Name
                th Status
            tbody
              each todo in todos
                tr
                  td=todo.id
                  td=todo.description
                  td=todo.completed
        form#uploadForm(action="/todos/photo", enctype="multipart/form-data", method="post")
          input#image_file(type="file", name="userPhoto", multiple)
          input(type="submit", value="Upload Image", name="submit")
          //- input#random(type="text", name="random")
          span#status
      div.animation
        button.canvas-stop(name ="Stop It") Stop It
        button.canvas-start(name ="Start It") Start All
        canvas#demoCanvas(width="960" height="550")